{% trans_default_domain 'FOSUserBundle' %}

    {% block stylesheets %}
        {{ encore_entry_link_tags('css/register') }}
    {% endblock %}

    <body class="text-center">
        <div class="form">
            <img class="mb-4" src="{{asset('build/images/logo.png')}}" alt="" width="100" height="100">
            <h1 class="h3 mb-3 font-weight-normal">Inscription</h1>
            {{ form_start(form, {'method': 'post', 'action': path('fos_user_registration_register'), 'attr': {'class': 'fos_user_registration_register'}}) }}
                {{ form_widget(form) }}
            <div>
                <input class="btn btn-lg btn-primary btn-block" id="_submit" name="_submit" type="submit" value="{{ 'registration.submit'|trans }}" />
            </div>
            {{ form_end(form) }}
        </div>
    </body>